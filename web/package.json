{
  "name": "specmate",
  "version": "v0.4.0-fix-1",
  "description": "Specmate for co-engineering requirements and tests",
  "scripts": {
    "extract": "ngx-translate-extract -i ./src -o ./src/assets/i18n/de.json ./src/assets/i18n/gb.json -m ngx_trans_extr --sort --format namespaced-json",
    "lint": "tslint ./src/**/*.ts -t verbose --exclude './src/app/model/**/*.ts'",
    "clean": "rimraf ../bundles/specmate-ui-core/webcontent/*",
    "build-dev": "npm run extract && node node_modules/@angular/cli/bin/ng build --configuration=dev",
    "build-prod": "npm run clean && node node_modules/@angular/cli/bin/ng build --configuration=prod",
    "dep-graph": "ngd -p src/tsconfig.json",
    "init": "npm prune && npm install",
    "license-check": "license-checker --summary --production",
    "update-check": "ncu",
    "check": "npm run lint && npm run license-check && npm run update-check",
    "update-dependencies": "ncu -u && npm run init"
  },
  "keywords": [],
  "author": "Qualicen GmbH",
  "license": "Apache-2.0",
  "dependencies": {
    "@angular/common": "8.2.13",
    "@angular/compiler": "8.2.13",
    "@angular/core": "8.2.13",
    "@angular/forms": "8.2.13",
    "@angular/platform-browser": "8.2.13",
    "@angular/platform-browser-dynamic": "8.2.13",
    "@angular/router": "8.2.13",
    "@ng-bootstrap/ng-bootstrap": "5.1.2",
    "@nguniversal/express-engine": "8.1.1",
    "@ngx-translate/core": "11.0.1",
    "@ngx-translate/http-loader": "4.0.0",
    "angular-split": "3.0.2",
    "angular2-uuid": "1.1.1",
    "bootstrap": "4.3.1",
    "bootswatch": "4.3.1",
    "classlist.js": "1.1.20150312",
    "core-js": "3.4.0",
    "detect-browser": "4.8.0",
    "file-saver": "2.0.2",
    "flag-icon-css": "3.4.5",
    "font-awesome": "4.7.0",
    "he": "^1.2.0",
    "jquery": "3.4.1",
    "mxgraph": "github:jgraph/mxgraph#12621211eed3495188c95bbc4b3020a1db451df5",
    "ng2-dragula": "2.1.1",
    "ngx-cookie": "4.1.2",
    "ngx-translate-core": "11.0.3",
    "ngx-translate-extract": "1.0.0",
    "popper.js": "1.16.0",
    "reflect-metadata": "0.1.13",
    "rxjs": "6.5.3",
    "rxjs-compat": "6.5.3",
    "typeface-open-sans": "0.0.75",
    "typeface-roboto": "0.0.75",
    "typeface-roboto-mono": "0.0.75",
    "zone.js": "0.10.2"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "0.803.18",
    "@angular/cli": "8.3.18",
    "@angular/compiler-cli": "8.2.13",
    "@biesbjerg/ngx-translate-extract": "5.0.0",
    "@types/file-saver": "2.0.1",
    "@types/he": "^1.1.0",
    "@types/jquery": "3.3.31",
    "@types/mxgraph": "github:lgleim/mxgraph-typings#474719ec8cedbb1ae5ea2cf121a41db105043374",
    "license-checker": "25.0.1",
    "npm-check-updates": "3.2.1",
    "rimraf": "3.0.0",
    "tslint": "5.20.1",
    "typescript": "3.4.5",
    "webpack": "4.41.2"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/junkerm/specmate/"
  }
}
